<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <Authors>Oliver Sanders</Authors>
    <Company />
    <Copyright>Oliver Sanders 2017</Copyright>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>CardGames.BeggarMyNeighbour</PackageId>
    <Product>CardGames.BeggarMyNeighbour</Product>
    <Description>Beggar My Neighbour Card Game Library</Description>
    <PackageProjectUrl>https://github.com/oli123456789/BeggarMyNeighbour</PackageProjectUrl>
